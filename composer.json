{
    "name": "restruct/edudex-api-client",
    "description": "PHP client library for the EDU-DEX Data API. Framework-agnostic with optional SilverStripe integration.",
    "type": "silverstripe-module",
    "keywords": [
        "edudex",
        "api-client",
        "silverstripe"
    ],
    "homepage": "https://github.com/restruct/edudex-api-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Restruct",
            "role": "Developer"
        },
        {
            "name": "Bart van Irsel/Webium",
            "role": "AI consultant/guru"
        },
        {
            "name": "Claude (Anthropic)",
            "role": "AI Co-Author"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^6.0",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "suggest": {
        "silverstripe/framework": "For SilverStripe integration (^3.7)",
        "monolog/monolog": "For enhanced logging support"
    },
    "extra": {
        "installer-name": "edudex-api-client"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
